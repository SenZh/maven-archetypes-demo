<archetype xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype/1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype/1.0.0
           http://maven.apache.org/xsd/archetype-1.0.0.xsd">
    <id>quickstart</id>
    <sources>
        <source>service/src/main/java/**/*.*</source>
        <source>ui/src/main/java/**/*.*</source>
    </sources>
    <testSources>
        <source>service/src/test/java/**/*.*</source>
        <source>ui/src/test/java/**/*.*</source>
    </testSources>
    <resources>
        <resource>service/src/main/resources/**/*.*</resource>
        <resource>ui/src/main/resources/**/*.*</resource>
    </resources>

    <testResources>
        <resource>service/src/test/resources/**/*.*</resource>
        <resource>ui/src/test/resources/**/*.*</resource>
    </testResources>
    <siteResources>
        <resource>ui/src/main/webapp/**/*.*</resource>
    </siteResources>
</archetype>